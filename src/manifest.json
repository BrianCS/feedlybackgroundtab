{
	"name": "Feedly Background Tab",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Open Feedly Links in Background Tab using key b",
	"content_scripts": [
		{
			"matches": ["http://www.feedly.com/*"],
			"js": ["js/keypress.js"]
		}
	],
	"background": { "scripts": ["js/background.js"] },
	"permissions": [
		"http://www.feedly.com/*"
	],
	"minimum_chrome_version": "21",
	"icons": { "128": "images/icon_128.png" }
}